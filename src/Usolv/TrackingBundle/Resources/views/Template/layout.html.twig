<!DOCTYPE html>
<html lang="en">
  {% block head %}
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="{{ asset('favicon.ico') }}" />

    <title>{% block title %}USOL-V Tracking Tool{% endblock %}</title>

    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="{{ asset('bundles/bootstrap/css/bootstrap.min.css') }}" />

    <!-- Custom styles for sticky footer navigation bar template -->
    <link rel="stylesheet" href="{{ asset('bundles/bootstrap/css/custom/sticky-footer-navbar.css') }}" />
    
    <!-- JQGrid CSS -->
    <!-- Loads the CSS code for the UI theme selected -->
    <link rel="stylesheet" media="screen" href="{{ asset('bundles/phpgrid-free-v1.5.2/lib/js/themes/ui-lightness/jquery-ui.custom.css') }}" />
    <!-- Loads the CSS settings for jqGrid -->
    <link rel="stylesheet" media="screen" href="{{ asset('bundles/phpgrid-free-v1.5.2/lib/js/jqgrid/css/ui.jqgrid.css') }}" />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    
    <!-- JQuery 1.11.2 Javascript -->
    <script src="{{ asset('/bundles/jquery/js/jquery-1.11.2.min.js') }}" type="text/javascript"></script>
    
    <!-- JQGrid 1.5.2 Javascript -->
    <script src="{{ asset('bundles/phpgrid-free-v1.5.2/lib/js/jquery.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/phpgrid-free-v1.5.2/lib/js/jqgrid/js/i18n/grid.locale-en.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/phpgrid-free-v1.5.2/lib/js/jqgrid/js/jquery.jqGrid.min.js') }}" type="text/javascript"></script>
    
    {% block head_script %}{% endblock %}
  {% endblock %}

  {% block body %}

    <!-- Fixed navbar -->
    {{ include('UsolvTrackingBundle:Template:header.html.twig') }}

    <!-- Begin page content -->
    <div class="container">
      {% block content %}{% endblock %}
    </div>
    <!-- End of page content -->

    <!-- Sticky footer -->
    {{ include('UsolvTrackingBundle:Template:footer.html.twig') }}
    
    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <!-- script src="bootstrap/js/bootstrap.min.js"></script -->
    <script src="{{ asset('bundles/bootstrap/js/bootstrap.min.js') }}"></script>
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="http://getbootstrap.com/assets/js/ie10-viewport-bug-workaround.js"></script>
  {% endblock %}
</html>
